<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="3dp" />
        </shape>
    </item>
    <item
        android:bottom="3dp"
        android:left="1dp"
        android:right="1dp"
        android:top="0dp">
        <shape android:shape="rectangle">
            <corners android:radius="3dp" />
            <solid android:color="@android:color/white" />
        </shape>
    </item>
</layer-list>